{
  "name": "covid-help",
  "version": "1.0.0",
  "description": "Website for Covid resources.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install npm && run build",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Mehul Varshney",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "mongoose": "^5.12.7"
  }
}
